<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- Tests -->
    <suppress checks="JavadocMethodCheck" files="^.*test"/>
    <suppress checks="JavadocVariableCheck" files="^.*test"/>
    <suppress checks="JavadocTypeCheck" files="^.*test"/>
    <suppress checks="MagicNumberCheck" files="^.*test"/>
    <suppress checks="MissingCtorCheck" files="^.*test"/>
    <suppress checks="MultipleStringLiteralsCheck" files="^.*test"/>
</suppressions>